#include <iostream>

#include <string.h>

int main()
{
	std::string animal;

	std::cin >> animal;

	std::string type;

	if (animal == "dog")
	{
		type = "mammal";
	}
	else if (animal == "snake")
	{
		type = "reptile";
	}
	else if (animal == "tortoise")
	{
		type = "reptile";
	}
	else if (animal == "crocodile")
	{
		type = "reptile";
	}
	else
	{
		type = "unknown";
	}

	std::cout << type << std::endl;

	return 0;
}
