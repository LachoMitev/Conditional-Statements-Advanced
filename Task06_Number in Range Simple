#include <iostream>

#include <string>


int main()
{

	int number;

	std::cin >> number;

	if (number == 0)
	{
		std::cout << "No" << std::endl;
	}

	else if (number > 100)
		{
			std::cout << "No" << std::endl;
		}

	else if (number < -100)
		{
			std::cout << "No" << std::endl;
		}

	else if (number !=0)
	{
		if (number >= -100)
		{
			std::cout << "Yes" << std::endl;
		}
		else if (number <= 100) 
		{
			std::cout << "Yes" << std::endl;
		}
		else
		{
			std::cout << "No" << std::endl;
		}
	}

	return 0;
}
